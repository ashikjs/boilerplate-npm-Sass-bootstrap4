{
  "name": "sass",
  "version": "1.0.0",
  "description": "I am going to learn SASS Program",
  "main": "index.js",
  "scripts": {
    "test": "node-sass --output-style compressed -o sass sass/main.sass public/css/main.css",
    "build-css": "node-sass --include-path sass sass/main.sass public/css/main.css && node-sass --include-path sass sass/bootstrap/bootstrap.scss  public/css/bootstrap.css",
    "watch-css": "nodemon -e scss -x 'npm run build-css'",
    "autoprefixer": "postcss -u autoprefixer -r public/css/main.css"
  },
  "keywords": [
    "SASS",
    "node",
    "css"
  ],
  "author": "ashikjs",
  "license": "ISC",
  "devDependencies": {
    "node-sass": "^4.6.1",
    "nodemon": "^1.12.1"
  },
  "dependencies": {
    "autoprefixer": "^7.1.6",
    "bootstrap": "^4.0.0-beta.2",
    "postcss-cli": "^4.1.1"
  }
}
